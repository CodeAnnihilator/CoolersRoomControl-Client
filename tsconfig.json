{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "lib": ["es2015", "esnext.asynciterable"],
    "jsx": "react-native",
	"strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "types": [
	  "jest",
	  "node",
	],
	"baseUrl": "src",
    "paths": {
      "assets/*": ["./assets/*"]
    }
  },
  "exclude": ["node_modules"]
}
